{
  "ConnectionStrings": {
    "Default": "User ID=DBuser;Password=DBpass;Host=localhost;Port=5432;Database=DBname;Pooling=true;"
  },
  "App": {
    "ServerRootAddress": "http://localhost:21021/",
    "ClientRootAddress": "http://localhost:4200/",
    "CorsOrigins": "http://localhost:4200,http://localhost:8080,http://localhost:8081,http://localhost:3000"
  },
  "Authentication": {
    "JwtBearer": {
      "IsEnabled": "true",
      "SecurityKey": "SECURITYKEY",
      "Issuer": "TalentV2",
      "Audience": "TalentV2"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:21021/"
      }
    }
  },
  "Swagger": {
    "ShowSummaries": false
  },
  "SeedData": {
    "AdminHostEmail": "admin@gmail.com",
    "DefaultTenantName": "Tenant",
    "DefaultTenantAdminEmail": "admin@gmail.com",
    "IsDefaultPasswordComplex": false
  },
  "KomuService": {
    "BaseAddress": "http://localhost:21021",
    "SecurityCode": "yourSecurityCode",
    "EnableKomuNotification": "true",
    "ChannelIdDevMode": "yourChannelIdDevMode" // channel dev-notification
  },
  "LMSService": {
    "BaseAddress": "http://localhost:21021",
    "FEAddress": "http://localhost:21021",
    "SecurityCode": "yourSecurityCode"
  },
  "AWS": {
    "Profile": "Profile",
    "AccessKeyId": "yourAccessKeyId",
    "SecretKeyId": "yourSecretKeyId",
    "Region": "ap-southeast-1",
    "BucketName": "bucketName",
    "Prefix": "dev",
    "CloudFront": "http://yourcloudfront.net"
  },
  "UploadFile": {
    "Provider": "InternalUploadFile", //InternalUploadFile, AWS
    "MaxSizeFile": "2000000"
  },
  "HRMService": {
    "BaseAddress": "http://localhost:21021",
    "SecurityCode": "yourSecurityCode"
  },
  "AppSettingProviders": {
    "App.UiTheme": "red",
    "App.GooleClientAppId": "yourGooleClientAppId",
    "App.StorageLocation": "",
    "App.KomuHRITChannelId": "",
    "App.KomuResourceRequestStaffChannelId": "",
    "App.KomuResourceRequestInternChannelId": "",
    "App.HRMURLSetting": "",
    "App.HRMSecurityCodeSetting": "",
    "App.ProjectURLSetting": "",
    "App.ProjectSecurityCodeSetting": "",
    "App.TimesheetURLSetting": "",
    "App.TimesheetSecurityCodeSetting": "",
    "App.TimesheetAutoUpdateSetting": "",
    "App.TalentSercurityCode": "yourSecurityCode",
    "App.TalentSetContest": "http://contest.nccsoft.vn/",
    "App.NoticeInterviewStartAtHour": "10",
    "App.NoticeInterviewEndAtHour": "17",
    "App.NoticeInterviewMinutes": "30",
    "App.NoticeInterviewResultMinutes": "120",
    "App.IsNoticeInterviewViaChannel": "true",
    "App.NoticeInterviewScheduleChannel": "",
    "App.NoticeInterviewResultChannel": "",
    "App.GoogleClientAppEnable": "",
    "App.EnableNormalLogin": "true"
  },
  "DefaultPassPhrase": ""
